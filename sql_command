CREATE TABLE past_delivery_data (id INTEGER PRIMARY KEY AUTOINCREMENT, serial INTEGER NOT NULL, model TEXT NOT NULL, HP_case_id TEXT NOT NULL, company_name TEXT NOT NULL, order_date INTEGER NOT NULL, arrive_date INTEGER NOT NULL, goods_receiver_name TEXT NOT NULL, address TEXT NOT NULL, goods_receiver_phone TEXT NOT NULL, goods_name TEXT NOT NULL, quantity INTEGER NOT NULL, remark TEXT, goods_type TEXT)